	rm -fR ${HOME}/ydfs/build-${ARCH}/*
	cp $HOME/ydfs/src/wxGTK-2.8.12/wx-config /tmp
	rm -fR ${HOME}/ydfs/src/*
	install -d $HOME/ydfs/src/wxGTK-2.8.12/
	cp /tmp/wx-config HOME/ydfs/src/wxGTK-2.8.12/
	rm -fR ${HOME}/ydfs/modules/*
	rm -fR ${HOME}/ydfs/packages-${ARCH}/*
	rm -fR ${HOME}/opkg
	rm -fR ${HOME}/ydfs/opkg
	rm -fR ${HOME}/ydfs/build/busybox-*
