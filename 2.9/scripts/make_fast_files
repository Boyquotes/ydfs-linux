cd $HOME
[ ! -e $HOME/iso/$ARCH.tar.gz ] && tar czvf $HOME/iso/$ARCH.tar.gz $ARCH
[ ! -e $HOME/iso/kernel-modules-$ARCH.tar.gz ] && tar czvf $HOME/iso/kernel-modules-$ARCH.tar.gz ydfs/kernel-$ARCH 
[ "$ARCH" = "x86_64" ] && [ ! -e $HOME/iso/multilib.tar.gz ] && tar czvf $HOME/iso/multilib.tar.gz multilib
[ ! -e $HOME/iso/kernel-$ARCH.tar.gz ] && tar czhvf $HOME/iso/kernel-$ARCH.tar.gz ydfs/build/linux-$ARCH-$KERNEL3/arch/$ARCH/boot/bzImage
echo OK
