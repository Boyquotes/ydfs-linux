cd $HOME/dibab/opkg
$HOME_DIBAB/tools/opkg-utils/opkg-make-index -v . > Packages
[ -e /var/www/opkg/ ] && cp -fR $HOME/dibab/opkg/* /var/www/opkg/$DISTRONAME
HOME_DIST=`basename $HOME_DIBAB`
[ -e /home/linuxconsole24/dibab/opkg ] && rsync --size-only -vr /home/linuxconsole24/dibab/opkg/ opkg@opkg.linuxconsole.org:www/linuxconsole/2.4/x86
echo "OK"
